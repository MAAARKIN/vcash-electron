{
  "name": "vcash-electron",
  "productName": "Vcash Electron GUI",
  "version": "0.33.8",
  "description": "Multi-platform and multi-node GUI for Vcash.",
  "main": "src/electron.js",
  "scripts": {
    "gui": "electron .",
    "dev": "NODE_ENV=dev electron .",
    "dev-win": "set NODE_ENV=dev&&electron .",
    "lint": "standard",
    "format": "prettier-standard 'src/**/*.js' && standard",
    "pack": "build --dir",
    "dist-linux": "build --linux --x64",
    "dist-macos": "build --mac --x64",
    "dist-win-nsis": "build --win nsis --ia32 --x64",
    "dist-win-ia32": "build --win portable --ia32",
    "dist-win-x64": "build --win portable --x64"
  },
  "build": {
    "appId": "vcash.electron.gui",
    "asarUnpack": [
      "build/bin"
    ],
    "linux": {
      "category": "Network",
      "target": [
        "deb",
        "zip"
      ]
    },
    "mac": {
      "category": "public.app-category.finance",
      "target": "dmg"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openvcash/vcash-electron.git"
  },
  "keywords": [
    "vcash",
    "electron",
    "gui"
  ],
  "author": "whphhg <whphhg@protonmail.ch>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/openvcash/vcash-electron/issues"
  },
  "homepage": "https://github.com/openvcash/vcash-electron#readme",
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "Audio"
    ]
  },
  "dependencies": {
    "antd": "2.13.4",
    "electron-compile": "6.4.2",
    "i18next": "9.0.1",
    "i18next-node-fs-backend": "1.0.0",
    "mobx": "3.3.1",
    "mobx-logger": "0.6.0",
    "mobx-react": "4.3.3",
    "moment": "2.18.1",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-i18next": "6.0.5",
    "react-router-dom": "4.2.2",
    "recharts": "0.22.4",
    "ssh2": "0.5.5"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-import": "1.6.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.26.0",
    "electron-builder": "19.34.1",
    "electron-prebuilt-compile": "1.7.8",
    "eslint": "3.19.0",
    "prettier-standard": "7.0.1",
    "standard": "10.0.3"
  }
}
